{
  "env": {
    "test": {
      "plugins": [
        ["istanbul", {
          "exclude": [
            "**/__mocks__/*.js",
            "**/__tests__/*-test.js"
          ]
        }]
      ]
    }
  },
  "plugins": [
    "dev-expression",
    "relay",
    "transform-object-rest-spread",
    "transform-class-properties",
  ],
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "8.5.0",
        },
      },
    ],
    "react"
  ]
}
