{
  "name": "server",
  "version": "0.0.1",
  "description": "Masochist server",
  "main": "dist/server/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/wincent/masochist.git"
  },
  "scripts": {
    "build": "gulp build",
    "check": "gulp typecheck",
    "coverage": "NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --opts mocha/mocha.opts 'src/**/__mocks__/*.js' 'src/**/__tests__/*-test.js'",
    "lint": "gulp lint",
    "pretty": "babel-node scripts/pretty.js",
    "pretty-check": "babel-node scripts/pretty.js --check",
    "relay": "relay-compiler --src ./src --schema schema.graphql",
    "start": "babel-node src/server/main.js",
    "start-prod": "NODE_ENV=production node dist/server/main.js",
    "test": "mocha --opts mocha/mocha.opts 'src/**/__mocks__/*.js' 'src/**/__tests__/*-test.js'",
    "update-indices": "babel-node src/bin/updateIndices.js",
    "update-schema": "babel-node src/bin/updateSchema.js"
  },
  "author": {
    "name": "Greg Hurrell",
    "email": "greg@hurrell.net",
    "url": "https://github.com/wincent"
  },
  "license": "CC-BY-NC-SA-4.0",
  "private": true,
  "devEngines": {
    "node": ">= 4.x",
    "npm": ">= 2.x"
  },
  "dependencies": {
    "assets-webpack-plugin": "*",
    "autoprefixer": "*",
    "babel-cli": "*",
    "babel-eslint": "*",
    "babel-loader": "*",
    "babel-plugin-dev-expression": "*",
    "babel-plugin-idx": "^1.5.1",
    "babel-plugin-relay": "dev",
    "babel-polyfill": "*",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "babel-preset-stage-0": "*",
    "babel-register": "*",
    "bluebird": "*",
    "body-parser": "*",
    "classnames": "*",
    "compiled-jade-render": "*",
    "css-loader": "*",
    "dataloader": "*",
    "entities": "*",
    "expect": "*",
    "express": "*",
    "express-graphql": "*",
    "extract-text-webpack-plugin": "*",
    "file-loader": "*",
    "flow-bin": "^0.46.0",
    "flow-typed": "^2.1.2",
    "fs-extra": "*",
    "glob": "^7.1.1",
    "graphql": "*",
    "graphql-relay": "*",
    "gulp": "*",
    "gulp-babel": "*",
    "gulp-jade": "*",
    "gulp-util": "*",
    "history": "*",
    "idx": "^1.5.0",
    "isomorphic-fetch": "*",
    "jade": "*",
    "marked": "*",
    "memcached": "*",
    "mocha": "*",
    "postcss": "*",
    "postcss-loader": "*",
    "prettier": "^1.3.1",
    "prop-types": "*",
    "react": "*",
    "react-dom": "*",
    "react-relay": "dev",
    "react-side-effect": "*",
    "redis": "*",
    "relay-compiler": "dev",
    "relay-runtime": "dev",
    "rss": "*",
    "scroll-behavior": "*",
    "style-loader": "*",
    "universal-router": "*",
    "unpack-content": "*",
    "url-loader": "*",
    "webpack": "*"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.3",
    "gulp-eslint": "*",
    "gulp-mocha": "^4.3.1",
    "gulp-shell": "*",
    "http-proxy": "*",
    "nyc": "^10.3.2",
    "sinon": "*",
    "webpack-dev-server": "*"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
