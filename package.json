{
  "name": "masochist",
  "version": "0.0.1",
  "description": "Masochist server",
  "main": "dist/server/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/wincent/masochist.git"
  },
  "config": {
    "content-repo": "."
  },
  "scripts": {
    "build": "gulp build",
    "check": "gulp typecheck",
    "coverage": "jest --forceExit --coverage",
    "gulp": "gulp",
    "lint": "gulp lint",
    "pretty": "babel-node scripts/pretty.js",
    "pretty-check": "babel-node scripts/pretty.js --check",
    "relay": "babel-node scripts/compile.js",
    "start": "MASOCHIST_CONTENT_REPO=$npm_package_config_content_repo babel-node src/server/main.js",
    "start-prod": "MASOCHIST_CONTENT_REPO=$npm_package_config_content_repo NODE_ENV=production node dist/server/main.js",
    "test": "jest --forceExit",
    "update-indices": "MASOCHIST_CONTENT_REPO=$npm_package_config_content_repo babel-node src/bin/updateIndices.js",
    "update-schema": "babel-node src/bin/updateSchema.js"
  },
  "author": {
    "name": "Greg Hurrell",
    "email": "greg@hurrell.net",
    "url": "https://github.com/wincent"
  },
  "license": "CC-BY-NC-SA-4.0",
  "private": true,
  "devEngines": {
    "node": ">= 8.x",
    "npm": ">= 2.x"
  },
  "dependencies": {
    "assets-webpack-plugin": "*",
    "autoprefixer": "^7.1.1",
    "babel-cli": "*",
    "babel-eslint": "*",
    "babel-loader": "^7.1.2",
    "babel-plugin-minify-replace": "0.2.0",
    "babel-plugin-relay": "^1.0.1",
    "babel-polyfill": "*",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "babel-preset-stage-0": "*",
    "babel-register": "*",
    "bluebird": "*",
    "body-parser": "^1.17.2",
    "classnames": "*",
    "css-loader": "*",
    "dataloader": "*",
    "entities": "*",
    "express": "^4.15.3",
    "express-graphql": "^0.6.5",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "*",
    "flow-bin": "^0.46.0",
    "flow-typed": "^2.1.2",
    "fs-extra": "*",
    "glob": "^7.1.2",
    "graphql": "*",
    "graphql-relay": "*",
    "gulp": "*",
    "gulp-babel": "*",
    "gulp-util": "*",
    "history": "*",
    "isomorphic-fetch": "*",
    "marked": "*",
    "memcached": "*",
    "postcss": "*",
    "postcss-loader": "*",
    "prettier": "^1.3.1",
    "prop-types": "^15.5.10",
    "react": "*",
    "react-dom": "*",
    "react-relay": "^1.0.0",
    "redis": "*",
    "relay-compiler": "^1.0.0",
    "relay-runtime": "^1.0.0",
    "rss": "*",
    "scroll-behavior": "*",
    "style-loader": "*",
    "universal-router": "*",
    "unpack-content": "*",
    "url-loader": "*",
    "webpack": "^3.6.0"
  },
  "devDependencies": {
    "gulp-eslint": "*",
    "gulp-shell": "*",
    "http-proxy": "*",
    "jest": "21.2.1",
    "webpack-dev-server": "^2.8.2"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "globals": {
      "__DEV__": true
    }
  },
  "prettier": {
    "bracket-spacing": false,
    "jsx-bracket-same-line": true,
    "parser": "flow",
    "single-quote": true,
    "trailing-comma": "all"
  }
}
